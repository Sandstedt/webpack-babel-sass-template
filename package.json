{
  "name": "webpack-babel-sass-template",
  "version": "1.0.0",
  "description": "Startup framework with webpack, babel, sass & autoprefixer",
  "main": "src/index.js",
  "engines": {
    "node": "5.11.0",
    "npm": "3.8.6"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js --mode production --progress --profile --colors -p --env.stage=production",
    "start": "webpack-dev-server --mode development --progress --profile --colors --config webpack.dev.js --env.stage=development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sandstedt/webpack-babel-sass-template"
  },
  "author": "Jonas Sandstedt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sandstedt/webpack-babel-sass-template/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^6.3.6",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.26.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
